-- Tab "Prehistoric Event": Auto Find Prehistoric Island

do
    local t = Tabs["Prehistoric Event"]
    t:MakeSeperator("=== Auto Prehistoric Island ü¶ñ ===")
     :Set("T·ª± ƒë·ªông t√¨m v√† d·ªãch chuy·ªÉn ƒë·∫øn Prehistoric Island")

    local autoFind = false
    local findTask

    local function notify(title, content)
        WazureV1:Notify({ Title = title, Content = content, Logo = "rbxassetid://18289959127", Time = 3 })
    end

    local function findPrehistoricIsland()
        for _, m in ipairs(Workspace:GetDescendants()) do
            if m:IsA("Model") and m.Name:lower():find("prehistoric island") then
                return m
            end
        end
    end

    local function startAutoFind()
        autoFind = true
        notify("Prehistoric Island", "ƒêang t√¨m Prehistoric Island...")

        findTask = task.spawn(function()
            while autoFind do
                local island = findPrehistoricIsland()
                if island and island.PrimaryPart then
                    notify("Prehistoric Island", "ƒê√£ t√¨m th·∫•y! D·ªãch chuy·ªÉn ƒë·∫øn ƒë·∫£o...")
                    LocalPlayer.Character:MoveTo(island.PrimaryPart.Position + Vector3.new(0, 10, 0))
                    autoFind = false
                    break
                end
                task.wait(2)
            end
        end)
    end

    local function stopAutoFind()
        autoFind = false
        if findTask then
            task.cancel(findTask)
            findTask = nil
        end
        notify("Prehistoric Island", "ƒê√£ d·ª´ng t√¨m ki·∫øm.")
    end

    t:MakeToggle("Auto Find Prehistoric Island", {
        Title = "T·ª± ƒë·ªông t√¨m ƒë·∫£o",
        Content = "T√¨m v√† d·ªãch chuy·ªÉn ƒë·∫øn Prehistoric Island",
        Default = false,
        Callback = function(isOn)
            if isOn then
                startAutoFind()
            else
                stopAutoFind()
            end
        end
    })
end

-- Tab "Prehistoric Event": Auto Complete Prehistoric Island Event

do
    local t = Tabs["Prehistoric Event"]
    t:MakeSeperator("=== Auto Complete Prehistoric Island ü¶ñ ===")
     :Set("T·ª± ƒë·ªông t√¨m v√† ho√†n th√†nh s·ª± ki·ªán Prehistoric Island")

    local autoComplete = false
    local completeTask

    local function notify(title, content)
        WazureV1:Notify({ Title = title, Content = content, Logo = "rbxassetid://18289959127", Time = 3 })
    end

    local function findPrehistoricIsland()
        for _, m in ipairs(Workspace:GetDescendants()) do
            if m:IsA("Model") and m.Name:lower():find("prehistoric island") then
                return m
            end
        end
    end

    local function startAutoComplete()
        autoComplete = true
        notify("Prehistoric Island", "ƒêang t√¨m Prehistoric Island...")

        completeTask = task.spawn(function()
            while autoComplete do
                local island = findPrehistoricIsland()
                if island and island.PrimaryPart then
                    notify("Prehistoric Island", "ƒê√£ t√¨m th·∫•y! D·ªãch chuy·ªÉn ƒë·∫øn ƒë·∫£o...")
                    LocalPlayer.Character:MoveTo(island.PrimaryPart.Position + Vector3.new(0, 10, 0))
                    task.wait(2)

                    -- T√¨m v√† t∆∞∆°ng t√°c v·ªõi Fossil Ancient Relic ƒë·ªÉ b·∫Øt ƒë·∫ßu s·ª± ki·ªán
                    local relic = island:FindFirstChild("Fossil Ancient Relic", true)
                    if relic and relic:IsA("BasePart") then
                        fireclickdetector(relic:FindFirstChildOfClass("ClickDetector"))
                        notify("Prehistoric Island", "ƒê√£ b·∫Øt ƒë·∫ßu s·ª± ki·ªán Volcano.")
                        task.wait(2)

                        -- Th·ª±c hi·ªán c√°c h√†nh ƒë·ªông c·∫ßn thi·∫øt ƒë·ªÉ ho√†n th√†nh s·ª± ki·ªán
                        -- V√≠ d·ª•: t·∫•n c√¥ng Lava Golems, gi·∫£m √°p su·∫•t n√∫i l·ª≠a, v.v.
                        -- B·∫°n c·∫ßn th√™m logic c·ª• th·ªÉ t·∫°i ƒë√¢y t√πy thu·ªôc v√†o y√™u c·∫ßu c·ªßa s·ª± ki·ªán

                        -- Sau khi ho√†n th√†nh s·ª± ki·ªán, thu th·∫≠p Dino Bones v√† Dragon Eggs
                        -- B·∫°n c·∫ßn th√™m logic c·ª• th·ªÉ t·∫°i ƒë√¢y ƒë·ªÉ t·ª± ƒë·ªông thu th·∫≠p v·∫≠t ph·∫©m

                        notify("Prehistoric Island", "ƒê√£ ho√†n th√†nh s·ª± ki·ªán v√† thu th·∫≠p v·∫≠t ph·∫©m.")
                        autoComplete = false
                        break
                    else
                        notify("Prehistoric Island", "Kh√¥ng t√¨m th·∫•y Fossil Ancient Relic.")
                    end
                end
                task.wait(2)
            end
        end)
    end

    local function stopAutoComplete()
        autoComplete = false
        if completeTask then
            task.cancel(completeTask)
            completeTask = nil
        end
        notify("Prehistoric Island", "ƒê√£ d·ª´ng ho√†n th√†nh s·ª± ki·ªán.")
    end

    t:MakeToggle("Auto Complete Prehistoric Island", {
        Title = "T·ª± ƒë·ªông ho√†n th√†nh s·ª± ki·ªán",
        Content = "T√¨m v√† ho√†n th√†nh s·ª± ki·ªán Prehistoric Island",
        Default = false,
        Callback = function(isOn)
            if isOn then
                startAutoComplete()
            else
                stopAutoComplete()
            end
        end
    })
end

-- Tab "Prehistoric Event": Auto Collect Dragon Eggs

do
    local t = Tabs["Prehistoric Event"]
    t:MakeSeperator("=== Auto Collect Dragon Eggs ü•ö ===")
     :Set("T·ª± ƒë·ªông nh·∫∑t Dragon Eggs sau s·ª± ki·ªán")

    local autoCollectEggs = false
    local collectEggsTask

    local function notify(title, content)
        WazureV1:Notify({ Title = title, Content = content, Logo = "rbxassetid://18289959127", Time = 3 })
    end

    local function startCollectEggs()
        autoCollectEggs = true
        notify("Prehistoric Island", "ƒêang ch·ªù nh·∫∑t Dragon Eggs...")

        collectEggsTask = task.spawn(function()
            while autoCollectEggs do
                for _, item in ipairs(Workspace:GetDescendants()) do
                    if item:IsA("Tool") and item.Name == "Dragon Egg" then
                        LocalPlayer.Character:MoveTo(item.Position)
                        task.wait(1)
                    end
                end
                task.wait(2)
            end
        end)
    end

    local function stopCollectEggs()
        autoCollectEggs = false
        if collectEggsTask then
            task.cancel(collectEggsTask)
            collectEggsTask = nil
        end
        notify("Prehistoric Island", "ƒê√£ d·ª´ng nh·∫∑t Dragon Eggs.")
    end

    t:MakeToggle("Auto Collect Dragon Eggs", {
        Title = "T·ª± ƒë·ªông nh·∫∑t tr·ª©ng r·ªìng",
        Content = "Nh·∫∑t Dragon Eggs sau s·ª± ki·ªán",
        Default = false,
        Callback = function(isOn)
            if isOn then
                startCollectEggs()
            else
                stopCollectEggs()
            end
        end
    })
end

-- Tab "Prehistoric Event": Auto Collect Dinosaur Bones

do
    local t = Tabs["Prehistoric Event"]
    t:MakeSeperator("=== Auto Collect Dinosaur Bones ü¶¥ ===")
     :Set("T·ª± ƒë·ªông nh·∫∑t Dinosaur Bones sau s·ª± ki·ªán")

    local autoCollectBones = false
    local collectBonesTask

    local function notify(title, content)
        WazureV1:Notify({ Title = title, Content = content, Logo = "rbxassetid://18289959127", Time = 3 })
    end

    local function startCollectBones()
        autoCollectBones = true
        notify("Prehistoric Island", "ƒêang ch·ªù nh·∫∑t Dinosaur Bones...")

        collectBonesTask = task.spawn(function()
            while autoCollectBones do
                for _, item in ipairs(Workspace:GetDescendants()) do
                    if item:IsA("Tool") and item.Name == "Dinosaur Bone" then
                        LocalPlayer.Character:MoveTo(item.Position)
                        task.wait(1)
                    end
                end
                task.wait(2)
            end
        end)
    end

    local function stopCollectBones()
        autoCollectBones = false
        if collectBonesTask then
            task.cancel(collectBonesTask)
            collectBonesTask = nil
        end
        notify("Prehistoric Island", "ƒê√£ d·ª´ng nh·∫∑t Dinosaur Bones.")
