-- 4) T·∫°o Tab "Music" v·ªõi TextInput ƒë·ªÉ nh·∫≠p URL
local currentMusic = nil
local sound = nil

-- H√†m ki·ªÉm tra ƒë·ªãnh d·∫°ng URL (ch·ªâ nh·∫≠n ID nh·∫°c Roblox)
local function isValidRobloxAudio(input)
    if string.find(string.lower(input), "youtube") or string.find(string.lower(input), "youtu.be") then
        return false
    end
    return string.match(input, "^%d+$") or string.find(input, "rbxassetid://")
end

-- H√†m ph√°t nh·∫°c
local function playMusic(url)
    if sound then
        sound:Destroy() -- X√≥a nh·∫°c c≈©
        sound = nil
    end

    -- T·∫°o Sound m·ªõi
    sound = Instance.new("Sound")
    sound.Parent = game:GetService("SoundService")
    sound.SoundId = url
    sound.Volume = 1
    sound.Looped = true
    sound:Play()

    print("ƒêang ph√°t nh·∫°c:", url)
endo

-- TextInput trong Tab "Music"
local MusicInput = MusicTab:MakeTextInput("Music", {
    ["Title"] = "Music",
    ["Content"] = "Add URL ID Roblox ƒë·ªÉ ph√°t nh·∫°c",
    ["Callback"] = function(value)
        if value ~= "" then
local MusicInput = MusicTab:MakeTextInput("Music", {
    ["Title"] = "Music",
    ["Content"] = "Add URL ID Roblox ƒë·ªÉ ph√°t nh·∫°c",
    ["Callback"] = function(value)
            if isValidRobloxAudio(value) then
                -- N·∫øu ch·ªâ l√† s·ªë, th√™m "rbxassetid://"
                if string.match(value, "^%d+$") then
                    value = "rbxassetid://" .. value
                end
                currentMusic = value
                playMusic(currentMusic)
            else
                -- Notify l·ªói n·∫øu nh·∫≠p link YouTube
                WazureV1:Notify({
                    Title = "L·ªói!",
                    Content = "Kh√¥ng ƒë∆∞·ª£c nh·∫≠p link YouTube! Ch·ªâ nh·∫≠n ID nh·∫°c Roblox!",
                    Logo = "rbxassetid://18289959127",
                    Time = 2,
                })
                print("URL nh·∫≠p sai ƒë·ªãnh d·∫°ng!")
            end
        else
            print("Kh√¥ng c√≥ URL ƒë∆∞·ª£c nh·∫≠p.")
        end
    end
})

