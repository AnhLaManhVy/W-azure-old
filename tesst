-- Tab "Leviathan"
-- ƒê·∫£m b·∫£o WazureV1 ƒë√£ ƒë∆∞·ª£c t·∫£i tr∆∞·ªõc khi th·ª±c thi (t·ª´ GitHub)
-- T·∫°o tab 'Leviathan' tr√™n giao di·ªán Wazure
local leviathanTab = WazureV1:CreateTab("Leviathan", "üåÄ")  -- B·∫°n c√≥ th·ªÉ thay ƒë·ªïi bi·ªÉu t∆∞·ª£ng n·∫øu c·∫ßn

-- Khai b√°o bi·∫øn l∆∞u tr·∫°ng th√°i v√† ƒë·ªëi t∆∞·ª£ng
local beastHunterBoat = nil       -- Thuy·ªÅn Beast Hunter t√¨m ƒë∆∞·ª£c
local bodyPosition = nil         -- BodyPosition gi·ªØ thuy·ªÅn ·ªü Y = 40
local randomMove = false         -- C·ªù ƒëi·ªÅu khi·ªÉn ch·∫ø ƒë·ªô bay ng·∫´u nhi√™n
local gateConnection = nil       -- K·∫øt n·ªëi s·ª± ki·ªán ChildAdded c·ªßa workspace

-- H√†m b·∫Øt ƒë·∫ßu Auto Find Leviathan
local function startAutoFind()
    -- T√¨m thuy·ªÅn 'Beast Hunter' trong workspace
    beastHunterBoat = workspace:FindFirstChild("Beast Hunter")
    if not beastHunterBoat then
        WazureV1:Notify("Auto Find Leviathan", "Kh√¥ng t√¨m th·∫•y thuy·ªÅn 'Beast Hunter'!", 3)
        return
    end

    -- T·∫°o BodyPosition ƒë·ªÉ gi·ªØ thuy·ªÅn ·ªü ƒë·ªô cao Y = 40
    local primaryPart = beastHunterBoat.PrimaryPart or beastHunterBoat:FindFirstChildWhichIsA("BasePart")
    if primaryPart then
        bodyPosition = Instance.new("BodyPosition", primaryPart)
        bodyPosition.Position = Vector3.new(primaryPart.Position.X, 40, primaryPart.Position.Z)
        bodyPosition.MaxForce = Vector3.new(1e5, 1e5, 1e5)  -- L·ª±c l·ªõn ƒë·ªß ƒë·ªÉ gi·ªØ thuy·ªÅn
    end

    -- V√¥ hi·ªáu h√≥a va ch·∫°m cho to√†n b·ªô th√†nh ph·∫ßn c·ªßa thuy·ªÅn (noclip)
    for _, part in ipairs(beastHunterBoat:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false
        end
    end

    -- B·∫Øt ƒë·∫ßu chu tr√¨nh di chuy·ªÉn ng·∫´u nhi√™n tr√™n m·∫∑t bi·ªÉn
    randomMove = true
    spawn(function()
        while randomMove do
            if not beastHunterBoat.PrimaryPart then break end
            -- Ch·ªçn to·∫° ƒë·ªô ng·∫´u nhi√™n (c√≥ th·ªÉ ƒëi·ªÅu ch·ªânh ph·∫°m vi cho ph√π h·ª£p)
            local randomX = math.random(-500, 500)
            local randomZ = math.random(-500, 500)
            local targetPos = Vector3.new(randomX, 40, randomZ)
            if bodyPosition then
                bodyPosition.Position = targetPos
            end
            wait(3)  -- Th·ªùi gian gi·ªØa c√°c l·∫ßn thay ƒë·ªïi v·ªã tr√≠
        end
    end)

    -- L·∫Øng nghe s·ª± ki·ªán xu·∫•t hi·ªán c·ªßa LeviathanGate trong workspace
    gateConnection = workspace.ChildAdded:Connect(function(child)
        if child.Name == "LeviathanGate" then
            -- Ph√°t hi·ªán LeviathanGate
            randomMove = false  -- D·ª´ng di chuy·ªÉn ng·∫´u nhi√™n
            -- Di chuy·ªÉn thuy·ªÅn t·ªõi g·∫ßn LeviathanGate
            local gatePrimary = child.PrimaryPart or child:FindFirstChildWhichIsA("BasePart")
            if gatePrimary and beastHunterBoat.PrimaryPart and bodyPosition then
                -- ƒê·∫∑t v·ªã tr√≠ c·ªßa BodyPosition g·∫ßn c·ªïng (v√≠ d·ª• c√°ch c·ªïng 10 ƒë∆°n v·ªã)
                bodyPosition.Position = gatePrimary.Position + Vector3.new(0, 0, 10)
            end
            -- G·ª≠i th√¥ng b√°o qua WazureV1
            WazureV1:Notify("Auto Find Leviathan", "ƒê√£ ph√°t hi·ªán LeviathanGate! ƒêang di chuy·ªÉn t·ªõi Leviathan.", 5)
        end
    end)
end

-- H√†m d·ª´ng Auto Find Leviathan
local function stopAutoFind()
    -- D·ª´ng di chuy·ªÉn ng·∫´u nhi√™n
    randomMove = false
    -- Ng·∫Øt k·∫øt n·ªëi s·ª± ki·ªán gi√°m s√°t LeviathanGate
    if gateConnection then
        gateConnection:Disconnect()
        gateConnection = nil
    end
    -- H·ªßy BodyPosition (n·∫øu c√≥)
    if bodyPosition then
        bodyPosition:Destroy()
        bodyPosition = nil
    end
    -- Kh√¥i ph·ª•c va ch·∫°m cho c√°c b·ªô ph·∫≠n c·ªßa thuy·ªÅn
    if beastHunterBoat then
        for _, part in ipairs(beastHunterBoat:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end
    -- Reset c√°c bi·∫øn
    beastHunterBoat = nil
    -- Th√¥ng b√°o ƒë√£ t·∫Øt ch·ª©c nƒÉng
    WazureV1:Notify("Auto Find Leviathan", "Ch·ª©c nƒÉng ƒë√£ ƒë∆∞·ª£c t·∫Øt.", 3)
end

-- T·∫°o Toggle "Auto Find Leviathan" s·ª≠ d·ª•ng WazureV1 API
leviathanTab:CreateToggle("Auto Find Leviathan", function(isEnabled)
    if isEnabled then
        startAutoFind()
    else
        stopAutoFind()
    end
end)
